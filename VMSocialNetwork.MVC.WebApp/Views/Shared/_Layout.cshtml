<html lang="pt-br">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <link rel="icon" type="image/png" href="~/Content/light_dashboard_v2/img/favicon.ico">

    <title>Light Bootstrap Dashboard by Creative Tim</title>

    <!-- Bootstrap core CSS     -->
    <link href="~/Content/light_dashboard_v2/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Animation library for notifications   -->
    <link href="~/Content/light_dashboard_v2/css/animate.css" rel="stylesheet" />

    <!--  Light Bootstrap Table core CSS    -->
    <link href="~/Content/light_dashboard_v2/css/light-bootstrap-dashboard.css?v=1.4.0" rel="stylesheet" />

    <!-- Fonts and icons -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>

    <link href="~/Content/light_dashboard_v2/css/pe-icon-7-stroke.css" rel="stylesheet" />
    
    <!-- Toastr -->
    <link href="~/Content/toastr/toastr.min.css" rel="stylesheet" />
    
    @RenderSection("styles", false)

</head>

<body>
    <div class="wrapper">

        <!-- Sidebar -->
        @RenderPage("~/Views/Shared/Sidebar.cshtml")
        <!-- End Sidebar -->

        <div class="main-panel">
            <!-- Navbar -->
            @RenderPage("~/Views/Shared/Navbar.cshtml")
            <!-- End Navbar -->
            <!-- Main  -->
            <div class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
            <!-- End Main -->
            <!-- Footer -->
            @RenderPage("~/Views/Shared/Footer.cshtml")
            <!-- End Footer -->

        </div>

    </div>

    <!--   Core JS Files   -->
    <script src="~/Content/light_dashboard_v2/js/core/jquery.3.2.1.min.js" type="text/javascript"></script>
    <script src="~/Content/light_dashboard_v2/js/core/popper.min.js" type="text/javascript"></script>
    <script src="~/Content/light_dashboard_v2/js/core/bootstrap.min.js" type="text/javascript"></script>

    <!--  Plugin for Switches, full documentation here: http://www.jque.re/plugins/version3/bootstrap.switch/ -->
    <script src="~/Content/light_dashboard_v2/js/plugins/bootstrap-switch.js"></script>

    <!--  Google Maps Plugin    -->
    @*<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>*@

    <!--  Chartist Plugin  -->
    <script src="~/Content/light_dashboard_v2/js/plugins/chartist.min.js"></script>

    <!--  Notifications Plugin    -->
    <script src="~/Content/light_dashboard_v2/js/plugins/bootstrap-notify.js"></script>

    <!-- Control Center for Light Bootstrap Dashboard: scripts for the example pages etc -->
    <script src="~/Content/light_dashboard_v2/js/light-bootstrap-dashboard.js?v=2.0.1" type="text/javascript"></script>

    <!-- Toastr -->
    <script src="~/Scripts/toastr/toastr.min.js"></script>


    <script type="text/javascript">
        $(document).ready(function () {

            // Javascript method's body can be found in assets/js/demos.js
            // demo.initDashboardPageCharts();
            // demo.showNotification();

        });
    </script>

    @RenderSection("scripts", false)

</body>
</html>